{
  "name": "objectsrepositoryserver",
  "version": "0.0.1",
  "description": "Server for ObjectsRepository",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc -p .",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "report": "jscpd -k 10 -l 2 src/",
    "prettier": "prettier src/**/*.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DH-Cologne/ObjectsRepositoryServer.git"
  },
  "jscpd": {
    "min-lines": 2,
    "min-tokens": 10,
    "reporters": [
      "html",
      "badge"
    ],
    "ignore": [
      "node_modules"
    ],
    "gitignore": true
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DH-Cologne/ObjectsRepositoryServer/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.ts'"
    }
  },
  "homepage": "https://github.com/DH-Cologne/ObjectsRepositoryServer#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "corser": "^2.0.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "flatten": "^1.0.3",
    "fs-extra": "^8.1.0",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^8.0.0",
    "klaw-sync": "^6.0.0",
    "mongodb": "^3.3.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^2.0.0",
    "passport": "^0.4.0",
    "passport-ldapauth": "^2.1.3",
    "passport-local": "^1.0.0",
    "shrink-ray-current": "^4.1.2",
    "slugify": "^1.3.6",
    "socket.io": "^2.3.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.16",
    "@types/fs-extra": "^8.0.1",
    "@types/imagemin": "^7.0.0",
    "@types/klaw": "^3.0.0",
    "@types/klaw-sync": "^6.0.0",
    "@types/mongodb": "^3.3.11",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.11",
    "@types/nodemailer": "^6.2.2",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "husky": "^4.0.0-beta.5",
    "jscpd": "^2.0.16",
    "jscpd-badge-reporter": "^1.1.3",
    "jscpd-html-reporter": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "optionalDependencies": {
    "node-zopfli-es": "~1.0.3"
  }
}
